#include<iostream>
#include <bits/stdc++.h>
using namespace std;

void IS(int arr[], int size){

for(int i=1; i<size; i++){
    int key = arr[i];
    int j = i-1
    while (key < array[j] && j >= 0) {
      array[j + 1] = array[j];
      --j;
    }
    array[j + 1] = key;
  }
}
 
}

int main(){

  int arr[5] ={2,7,5,1,0};

  IS(arr,5);

  for(int i=0; i<5; i++){
    cout<<arr[i]<<" ";
  }

  return 0;
}
